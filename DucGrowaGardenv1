                task.wait(0.5)

                -- ·∫§n prompt ƒë·ªÉ m·ªü GUI
                fireproximityprompt(prompt)
                print("‚úÖ ƒê√£ m·ªü b·∫£ng Seed GUI")
            else
                warn("‚ö†Ô∏è Kh√¥ng t√¨m th·∫•y v·ªã tr√≠ ho·∫∑c prompt.")
            end
        end)
    end
})
