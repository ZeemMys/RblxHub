local harvesting = false

local isSelling = false -- Biến kiểm soát trạng thái toggle, bắt đầu ở trạng thái "Tắt"

local function toggleSell() 
    isSelling = not isSelling  -- Đảo ngược trạng thái toggle mỗi khi gọi
    if isSelling then
        -- Nếu đang bật chế độ bán đồ, hiển thị các lựa chọn
        Top_Text_upvr.NpcText(Parent_upvr, "Got anything to sell?", false)
    else
        -- Nếu đang tắt chế độ bán đồ, hủy bỏ các lựa chọn và thông báo
        Top_Text_upvr.NpcText(Parent_upvr, "Nevermind", false)
    end
end

